@using Website.Core
@model Restaurant

<tr>
    <td>@Model.Name</td>
    <td>@Model.Location</td>
    <td>@Model.Cuisine</td>
    <td>
        <a asp-page="./Detail"
           asp-route-restaurantId="@Model.Id">
            Detail
        </a>
        <a asp-page="./Edit"
           asp-route-restaurantId="@Model.Id">
            Edit
        </a>
        <a asp-page="./Delete"
           asp-route-restaurantId="@Model.Id">
            Delete
        </a>
    </td>
</tr>

